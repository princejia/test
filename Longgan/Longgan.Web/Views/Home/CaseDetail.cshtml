@{
    ViewBag.Title = "CaseDetail";
}

<link rel="stylesheet" type="text/css" href="~/Content/rs-plugin/css/settings.css" media="screen" />


<div id="wrapper">

    <div class="singleheader">
        <div class="container">
            <div class="col-lg-6 col-sm-6 col-md-6">
